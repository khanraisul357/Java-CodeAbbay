import java.util.Scanner;
public class Main{
    public static void main(String []args){
      Scanner input=new Scanner(System.in);

      int arr_size=input.nextInt();
      int arr[]=new int[arr_size];
      int sum=0;

      for(int i=0;i<arr_size;i++){
         arr[i]=input.nextInt();
      }
       
      for(int i=0;i<arr_size;i++){
        sum=sum+arr[i];
     }

     System.out.println(sum);

     input.close();

     }
}
